<html>
  <head>
    <% include partials/head.ejs %>
  </head>

  <body>
    <% include partials/nav.ejs %>
    <div class="container">
      <div class="row">
        <div class="col s12 m12 l8">
          <img class="materialboxed" width="100%" src="../<%= project.images[0] %>">
        </div>
        <div class="col s12 m12 l4">
          <h4><%= project.title %></h4>
          <p><%= project.description %></p>
        </div>
        <% for(var i = 1; i < project.images.length; i++) {%>
          <div class="col s12 m12 l8">
            <img class="materialboxed" width="100%" src="../<%= project.images[i] %>">
          </div>
        <% } %>
      </div>
    </div>
  </body>
</html>
